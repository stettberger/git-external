[external "is-positive-git"]
	path = is-positive-git
	url = https://github.com/stettberger/ispositive.git
	branch = master
	vcs = git

[external "is-positive-symlink"]
	path = is-positive-symlink
	url = https://github.com/stettberger/ispositive.git
	symlink = is-positive-git
	vcs = git

[external "is-positive-hash"]
	path = is-positive-hash
	url = https://github.com/stettberger/ispositive.git
	branch = 92bb8b7949e3062d0768dcad0a493210782d8f6b
	vcs = git

[external "is-positive-sparse"]
	path = is-positive-sparse
	url = https://github.com/stettberger/ispositive.git
	branch = master
	vcs = git
        cloneArgs  = "--sparse --depth=1"
        updateArgs = --stat
